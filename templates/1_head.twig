<head>
	<!-- Méta-données du document -->
	<meta charset="utf-8" />
	<meta name="Author" content="Florian Trayon" />
	<meta name="description" content="{{ head_description }}" />
	<meta name="keywords" lang="{{ head_language }}" content="{{ head_keywords }}" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />

	<meta property="og:type" content="website" />
	<meta property="og:url" content="{{ url }}" />
	<meta property="og:title" content="Presidential Notes" />
	<meta property="og:description" content="{{ head_description }}" />
	<meta property="og:image" content="github_opengraph_url" />

	<meta property="twitter:card" content="summary_large_image" />
	<meta property="twitter:url" content="{{ url }}" />
	<meta property="twitter:title" content="Presidential Notes" />
	<meta property="twitter:description" content="{{ head_description }}" />
	<meta property="twitter:image" content="github_opengraph_url" />

	<!-- Titre du document -->
	<title>Presidential Notes</title>

	<!-- Pré-connexion des ressources externes -->
	<link rel="preconnect" href="https://cdnjs.cloudflare.com" />

	<!-- Polices & feuilles de style CSS -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/8.0.1/normalize.min.css" crossorigin="anonymous" />
	<link rel="stylesheet" href="styles/{{ file }}.css" />

	<!-- Scripts JavaScript -->
	<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
	<script src="scripts/{{ file }}.js"></script>
</head>